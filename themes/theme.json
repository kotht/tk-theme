{
    "name": "TK Theme",

    "type": "dark",
    "colors": {
        "editor.background": "#202020",
        "editor.foreground": "#a9b1d6",
        "editor.selectionBackground": "#515c7e",
        "editor.selectionForeground": "#c0caf5",
        "editor.lineHighlightBackground": "#292e42",
        "editorCursor.foreground": "#c0caf5",
        "editorCursor.background": "#a9b1d6",
        "editorLineNumber.foreground": "#565f89",
        "editorLineNumber.activeForeground": "#737aa2",
        "editorHint.foreground": "#7dcfff",
        "editorHint.border": "#292e42",
        "editor.findMatchBackground": "#3d59a1",
        "editor.findMatchForeground": "#c0caf5",
        "editor.findMatchHighlightBackground": "#3d59a1",
        "editor.findMatchHighlightForeground": "#c0caf5",
        "statusBar.background": "#1a1b26",
        "statusBar.foreground": "#a9b1d6",
        "sideBar.background": "#202020",
        "sideBar.foreground": "#a9b1d6",
        "titleBar.activeBackground": "#202020",
        "titleBar.activeForeground": "#a9b1d6"
    },

    "tokenColors": [
        {

            "scope": ["comment"],
            "settings": {
                "foreground": "#565f89"
            }
        },

        {
            "scope": ["string"],
            "settings": {
                "foreground": "#9ece6a"
            }
        },
        {
            "scope": ["keyword"],
            "settings": {
                "foreground": "#bb9af7"
            }
        },
        {
            "scope": ["variable"],

            "settings": {

                "foreground": "#7aa2f7"

            }
        },
        {
            "scope": ["constant"],
            "settings": {
                "foreground": "#ff9e64"
            }
        },
        {
            "scope": ["entity.name.function"],

            "settings": {

                "foreground": "#7aa2f7"

            }
        },
        {
            "scope": ["storage.type"],
            "settings": {
                "foreground": "#bb9af7"
            }
        },
        {
            "scope": ["entity.name.type"],

            "settings": {

                "foreground": "#c0caf5"
            }
        },
        {
            "scope": ["support.class"],
            "settings": {
                "foreground": "#7dcfff"
            }
        }
    ]
}
